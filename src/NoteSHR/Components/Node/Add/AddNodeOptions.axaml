<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:models="clr-namespace:NoteSHR.Core.Models"
             xmlns:add="clr-namespace:NoteSHR.Components.Node.Add"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NoteSHR.Components.Node.Add.AddNodeOptions"
             x:CompileBindings="False">

    <UserControl.DataContext>
        <add:AddNodeOptionsViewModel />
    </UserControl.DataContext>

    <ItemsControl ItemsSource="{Binding Components}">
        <ItemsControl.ItemTemplate>
            <DataTemplate x:DataType="models:ComponentType">
                <Button Click="Button_OnClick" Foreground="White" Content="{Binding DisplayName}" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>

</UserControl>